{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:general": "yarn workspace general dev",
    "build": "run-p next build:*",
    "build:general": "yarn workspace general:build",
    "lint": "run-s lint:*",
    "lint:general": "yarn workspace general lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"./packages/**/src/**/*.{ts,tsx}\"",
    "generate": "yarn workspace general graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@chakra-ui/icons": "1.1.5",
    "@chakra-ui/react": "1.8.3",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@kintone/rest-api-client": "2.0.34",
    "dayjs": "1.10.7",
    "framer-motion": "6.2.6",
    "graphql": "^16.3.0",
    "next": "12.0.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.0",
    "recoil": "0.6.1",
    "swr": "1.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.5",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@types/node": "16.11.24",
    "@types/react": "17.0.39",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "npm-run-all": "4.1.5",
    "pathpida": "0.17.0",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  }
}
