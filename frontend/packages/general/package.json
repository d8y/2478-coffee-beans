{
  "name": "general",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch --enableStatic",
    "dev:graphql": "graphql-codegen --watch",
    "build": "pathpida --ignorePath .gitignore --enableStatic && next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.jsx,.tsx --ignore-path .lintignore . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "generate": "graphql-codegen --config codegen.yml"
  }
}